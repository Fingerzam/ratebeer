<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @beer_club.name %>
</p>

<p>
  <b>Founded:</b>
  <%= @beer_club.founded %>
</p>

<p>
  <b>City:</b>
  <%= @beer_club.city %>
</p>

<p>
  <b>Members:</b>
  <ul>
    <% for m in @beer_club.memberships %>
      <li>
      <%= link_to m.user.to_s, m.user %>
      </li>
    <% end %>
  </ul>
</p>


<%= link_to 'Edit', edit_beer_club_path(@beer_club) %> |
<%= link_to 'Back', beer_clubs_path %>
